{
  "name": "@larner.dev/api",
  "description": "A larner.dev project",
  "version": "1.0.2",
  "author": "Aaron Larner",
  "type": "module",
  "scripts": {
    "build": "npm run build-cjs && npm run build-esm && npm run build-types",
    "build-cjs": "esbuild src/index.ts --bundle --platform=node --target=node14 --outdir=build/cjs  --packages=external --format=cjs",
    "build-esm": "esbuild src/index.ts --bundle --platform=node --target=node14 --outdir=build/esm  --packages=external --format=esm",
    "build-types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly --outfile build/types.d.ts",
    "dev": "npm run build-esm -- --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "test": "jest"
  },
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "types": "./build/types.d.ts"
    }
  },
  "types": "./build/types.d.ts",
  "main": "./build/esm/index.js",
  "devDependencies": {
    "@types/jest": "29.2.5",
    "@types/koa__cors": "3.3.0",
    "@types/koa-bodyparser": "4.3.10",
    "@types/mime-types": "2.1.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "esbuild": "0.16.16",
    "esbuild-jest": "0.5.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "jest": "29.3.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@koa/cors": "4.0.0",
    "@larner.dev/perk-response-codes": "1.0.0",
    "dotenv": "16.0.3",
    "koa": "2.14.1",
    "koa-bodyparser": "4.3.0",
    "mime-types": "2.1.35",
    "path-to-regexp": "6.2.1"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
